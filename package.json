    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/css-mediaquery": "^0.1.1",
    "@types/enzyme": "^3.10.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "enzyme": "^3.11.0",
    "jest-enzyme": "^7.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/components/**/*.{ts,tsx}",
      "!**/src/components/**/*.{stories,styles,types,actions}.{ts,tsx}",
      "!**/src/components/**/index.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
